    console.log(userInput); 

    socket.emit('manualMode', { winnobyadmin:userInput});
  });
  cammandMode.addEventListener('click', function() {
    console.log('cammandMode');
    socket.emit('manualMode', { winnobyadmin:undefined});
  });
});


// let gamePercent=document.getElementById('gamePercent').value;
const rouletteRate = {
    Straight: 36,
    Split: 18,
    Street: 12,
    Corner: 9,
    DoubleStreet: 6,
    Basket: 3,
    FiveBet: 3,
    Column: 3,
    FirstRow: 3,
    SecondRow: 3,
    ThirdRow: 3,
    FirstDozen: 3,
    SecondDozen: 3,
    ThirdDozen: 3,
    Low: 2,
    High: 2,
    Odd: 2,
    Even: 2,
    Red: 2,
    Black: 2,
};
socket.emit('sendtimeAdmin', {  msg:"hi" });

socket.on('SENDWIN', function (data) {
    // console.log(data,"kjshfdkash");
    if(data.winNumber==0){
        data.winNumber="00"
    }else{
        data.winNumber=data.winNumber-1
    }
    setTimeout(function () {
        swal({
            icon:  "success" ,
            title: `Win Number Is ${data.winNumber}`
        });
    }, 13000);
});
socket.on('sendtimeAdmin', function (data) {
    startCountdown(data, document.getElementById('countdown')); 
});
let profit=0;
socket.on('OnChipMoveRoullate', function (data) {
    console.log(data);
    let totalChips = 0;
    let uniqueUsers = new Set();
    let userTotalBet = {};    
    let userStraightSpot = {};
    let userSplitSpot = {};   
    let userStreetSpot = {};   
    let userDoubleStreetSpot = {};  
    let userCornerSpot = {};  
    let userThreeSpot = {};   
    let userLowSpot = {};     
    let userHighSpot = {};    
    let userOddSpot = {};     
    let userEvenSpot = {};    
    let userRedSpot = {};     
    let userBlackSpot = {};  

    let typeData = {};
    let chipValues = {};
    let returnAmount = {};
    let profitPercent = {};

    if (Object.keys(data.bets).length === 0) {
        document.querySelectorAll('p').forEach(p => {
            p.innerText = '0';
        });
        document.querySelectorAll('zero').forEach(p => {
            p.innerText = '0';
        });
        profit=0
        startCountdown(59, document.getElementById('countdown')); 
        // manualModeForm.style.display = 'none'; 
    } else {
        Object.values(data.bets).forEach(bet => {
            bet.forEach(b => {
                let gamePercent= rouletteRate[b.type]
                // console.log(gamePercent);

                totalChips += b.chip;
                uniqueUsers.add(b.playerId); 
                
                userTotalBet[b.playerId] = (userTotalBet[b.playerId] || 0) + b.chip;

                typeData[b.type]=(typeData[b.type] || 0) + 1;
                if (rouletteRate[b.type] == 3) {
                      let realThreeSpot=parseInt(b.spot)-1;
                    userThreeSpot[b.playerId] = (userThreeSpot[b.playerId] || '') + b.spot + ',';
                }
                switch (b.type) {
                    case 'Straight':
                        let realStraightSpot=parseInt(b.spot)-1;
                       console.log(b.spot);
                       if(realStraightSpot==-1){
                        realStraightSpot="00"
                       }
                        userStraightSpot[b.playerId] = (userStraightSpot[b.playerId] || '') + realStraightSpot + ',';
                        break;
                        case 'Split':
                            // console.log(b.spot);
                            let splitSpot;
                            let numbersArray;
                            if (typeof b.spot === 'string') {
                                splitSpot = b.spot.replace(/,$/, ''); 
                                numbersArray = splitSpot.split(',').map(num => parseInt(num) - 1); 
                            } else if (Array.isArray(b.spot)) {
                                numbersArray = b.spot.map(num => parseInt(num) - 1); 
                            }
                            userSplitSpot[b.playerId] = numbersArray.join(','); 
                            break;


                    case 'Corner':
                            let cornerSpot = b.spot.replace(/,$/, ''); 
                            let numbersArray2 = cornerSpot.split(',').map(num => parseInt(num) - 1); 
                        userCornerSpot[b.playerId] = numbersArray2.join(','); 
                        break;

                    case 'Street':
       
                        let streetSpot = b.spot.replace(/,$/, ''); 
                        let numbersArray3 = streetSpot.split(',').map(num => parseInt(num) - 1); 
                        userStreetSpot[b.playerId] = numbersArray3.join(',');
                        break;
                    case 'DoubleStreet':
                    let DoubleStreetSpot = b.spot.replace(/,$/, ''); 
                        let numbersArray4 = DoubleStreetSpot.split(',').map(num => parseInt(num) - 1); 
                        userDoubleStreetSpot[b.playerId] = numbersArray4.join(',');
                        break;

                    case 'Low':
                    let LowSpot = b.spot.replace(/,$/, ''); 
                        let numbersArray5 = LowSpot.split(',').map(num => parseInt(num) - 1); 
                        userLowSpot[b.playerId] = numbersArray5.join(',');
                        break;
                    case 'High':
                    let HighSpot = b.spot.replace(/,$/, ''); 
                        let numbersArray6 = HighSpot.split(',').map(num => parseInt(num) - 1); 
                        userHighSpot[b.playerId] = numbersArray6.join(',');
                        break;
                    case 'Odd':
                    let OddSpot = b.spot.replace(/,$/, ''); 
                        let numbersArray7 = OddSpot.split(',').map(num => parseInt(num) - 1); 
                        userOddSpot[b.playerId] = numbersArray7.join(',');
                        break;
                    case 'Even':
                        let EvenSpot = b.spot.replace(/,$/, ''); 
                        let numbersArray8 = EvenSpot.split(',').map(num => parseInt(num) - 1); 
                        userEvenSpot[b.playerId] = numbersArray8.join(',');
                        break;
                    case 'Red':
                        let RedSpot = b.spot.replace(/,$/, ''); 
                        let numbersArray9 = RedSpot.split(',').map(num => parseInt(num) - 1); 
                        userRedSpot[b.playerId] = numbersArray9.join(',');
                        break;
                    case 'Black':
                        let BlackSpot = b.spot.replace(/,$/, '');
                        let numbersArray10 = BlackSpot.split(',').map(num => parseInt(num) - 1);
                        userBlackSpot[b.playerId] = numbersArray10.join(',');
                        break;
                }

                let spots = Array.isArray(b.spot) ? b.spot : b.spot.split(',').filter(spot => spot !== '');
                spots.forEach(spot => {
                    spot = spot - 1; 
                    // console.log(spot);
                    chipValues[spot] = (chipValues[spot] || 0) + b.chip;
                    const rate = gamePercent/100; 
                    returnAmount[spot] = (returnAmount[spot] || 0) + (b.chip * rate);
                    profitPercent[spot] = (returnAmount[spot] || 0) + (b.chip * rate);
                    
                    if(spot==-1){
                    spot="00"
                    chipValues[spot] = (chipValues[spot] || 0) + b.chip;
                    const rate = gamePercent/100; 
                    returnAmount[spot] = (returnAmount[spot] || 0) + (b.chip * rate);

                    profitPercent[spot] = (returnAmount[spot] || 0) + (b.chip * rate);
                    }
                   
                });
            });
        });

        updateUserTable();

        async function updateUserTable () {
            const userDataContainer = document.querySelector('#userData tbody');
            userDataContainer.innerHTML = ''; 
            let count = 0;
            let name;
            uniqueUsers.forEach(async userId => {
                count++;
               
                let name;
                let realName= await SendName(userId).then(data => {
                    name=data.data
                });
                const userData = `
                    <tr>
                        <td><p>${count}</p></td>
                        <td><p>${name}</p></td>
                        <td><p>${userStraightSpot[userId] || '0'}</p></td>
                        <td><p>${userSplitSpot[userId] || '0'}</p></td>
                        <td><p>${userStreetSpot[userId] || '0'}</p></td>
                        <td><p>${userCornerSpot[userId] || '0'}</p></td>
                        <td><p>${userDoubleStreetSpot[userId] || '0'}</p></td>
                        <td><p>${userThreeSpot[userId] || '0'}</p></td>
                        <td><p>${userLowSpot[userId] || '0'}</p></td>
