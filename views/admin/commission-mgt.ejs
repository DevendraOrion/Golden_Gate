    const saveSettingsCompany = () => {
      // const Roullete = $('#Roullete').val();
      // const type = $('#type').val();
      // const Avaitor = $('#Avaitor').val();
      // const CarRoullete = $('#CarRoullete').val();
      const Roullete = document.getElementById("Roullete").value;
      const Avaitor = document.getElementById("Avaitor").value;
      const CarRoullete = document.getElementById("CarRoullete").value;
      // const type = document.getElementById("type").value;
      const type = document.getElementById("type").getAttribute("data-type"); // Get the type from the data-type attribute
console.log(CarRoullete, Avaitor, Roullete,type);
      fetch("/admin/saveCommission-mgt", {
        method: "POST",
        body: JSON.stringify({ CarRoullete, Avaitor, Roullete,type }),
        headers: {
          "Content-Type": "application/json",
        },
      })
      .then(response => response.json())
        .then(response => {
          const title = response.status == 1 ? "Success" : "Error";
          swal({
            icon: response.status == 1 ? "success" : "error",
            title: response.Msg
          }).then(() => {
            window.location.reload();
          });
        })
        .catch((error) => {
          console.log(error);
          swal({ icon: "error", title: "An error occurred" }).then(() => {
            window.location.reload();
          });
        });
